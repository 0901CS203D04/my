<!-- login.ejs -->
<html>
  <head>
    <title>User Login</title>
  </head>
  <body>
    <h1>User Login</h1>
    <form method="POST" action="/logins">
      <div>
        <label>Email:</label>
        <input type="email" name="email" required>
      </div>
      <div>
        <label>Password:</label>
        <input type="password" name="password" required>
      </div>
      <div>
        <button type="submit">Logins</button>
      </div>
    </form>
  </body>
</html>
